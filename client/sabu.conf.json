{
    "source": "./dist",
    "host": "0.0.0.0",
    "port": 3000,
    "cors": {
        "preflightMaxAge": 5,
        "origins": ["*"],
        "allowHeaders": [
            "X-Access-Token",
            "Access-Control-Allow-Origin",
            "Access-Control-Allow-Methods",
            "Access-Control-Allow-Headers"
        ],
        "exposeHeaders": ["API-Token-Expiry"]
    },
    "routes": {
        "/": "dist/index.html"
    }
}